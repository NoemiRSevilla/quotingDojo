@using quotingDojo.Models
@model Quote
<link rel="stylesheet" href="~/css/site.css"/>
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

<nav class="navbar navbar-expand-lg navbar-light bg-dark">
    <h1 class="white mr-auto">Here are the awesome quotes!</h1>
</nav>
<body>
    <div class="bigbox container">
        @foreach(var quote in ViewBag.AllQuotes)
        {
            <div class="container quoteinfo">
                <h3 class="row"> @quote["_Quote"]</h3>
                <h3 class="row">- @quote["Name"] at @quote["created_at"].ToString("h:mm tt MMM d yyyy")</h3>
            </div>
        }
    </div>
</body>